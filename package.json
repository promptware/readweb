{
  "name": "readweb",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "esbuild src/**/*.ts --bundle --platform=node --format=esm --outdir=dist --sourcemap",
    "dev": "esbuild src/**/*.ts --bundle --platform=node --format=esm --outdir=dist --sourcemap --watch",
    "lint": "eslint .",
    "test": "tsx --test test/**/*.test.ts",
    "run:fixture": "tsx src/scripts/runFixture.ts",
    "clean:fixture": "tsx src/scripts/cleanFixture.ts"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@types/node": "^24.7.0",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/jsdom": "^27.0.0",
    "ai": "^5.0.60",
    "chalk": "^5.3.0",
    "cheerio": "^1.1.2",
    "dotenv": "^17.2.3",
    "entities": "^7.0.0",
    "firecrawl": "^4.3.8",
    "jsdom": "^27.0.0",
    "newtype-ts": "^0.3.5",
    "openai": "^6.1.0",
    "rehype-format": "^5.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-remark": "^10.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-stringify": "^11.0.0",
    "ts-pattern": "^5.8.0",
    "unified": "^11.0.5",
    "unist-util-remove": "^4.0.0",
    "zod": "^4.1.11"
  }
